{
  "name": "expressjs_rest_api_socket.io_type_module",
  "version": "1.0.0",
  "description": "A boilerplate/starter project for quickly building RESTful APIs and Socket.IO using Node.js, Express, and MySQL.",
  "packageManager": "yarn@3.3.0",
  "languageName": "node",
  "type": "module",
  "module": "./src/index.mjs",
  "repository": "https://github.com/MASabbe/rest-api-socket.io-expressJs-boilerplate.git",
  "author": "mas.abbe@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "express-js",
    "ESM",
    "module",
    "RESTful API",
    "Socket.io",
    "boilerplate"
  ],
  "scripts": {
    "test": "jest .",
    "lint": "eslint ./src/** --ext .mjs",
    "lint:fix": "eslint ./src/** --ext .mjs --fix",
    "docs": "apidoc -i ./src/ -o ./public/docs/",
    "postdocs": "open-cli ./public/docs/index.html",
    "dev": "nodemon babel-node ./src/index.mjs",
    "clean": "rm -rf dist",
    "build": "yarn run clean && babel ./src --out-dir dist --copy-files",
    "prod": "pm2 start ./ecosystem.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "apidoc": "^0.53.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validation": "^4.1.0",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "joi": "^17.7.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "method-override": "^3.0.0",
    "mongoose": "^6.7.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.5.1",
    "redis-om": "^0.3.6",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.4",
    "winston": "^3.8.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  }
}
